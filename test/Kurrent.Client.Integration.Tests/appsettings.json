{
  "Kurrent": {
    "Client": {
      "Tests": {
        "AutoWireUpContainers": false,
        "ContainerImage": "docker.kurrent.io/kurrent-preview/kurrentdb:25.0.1-experimental-arm64-8.0-jammy"
      }
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Grpc.Net.Client.Internal": "Error",
        "EventStore.Client.SharingProvider": "Error"
      }
    },
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  }
}
